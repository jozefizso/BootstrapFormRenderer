{
	"name": "jozefizso/bootstrap-form-renderer",
	"type": "library",
	"description": "Nette forms renderer with bootstrap",
	"keywords": ["nette", "kdyby", "forms", "bootstrap"],
	"homepage": "https://github.com/jozefizso/BootstrapFormRenderer",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Filip ProchÃ¡zka",
			"homepage": "http://filip-prochazka.com",
			"email": "filip@prochazka.su"
		},
		{
			"name": "Jozef Izso",
			"email": "jozef.izso@gmail.com"
		}
	],
	"support": {
		"issues": "https://github.com/jozefizso/BootstrapFormRenderer/issues"
	},
	"require": {
		"php": ">=5.6",
		"latte/latte": "~2.3.0",
		"nette/application": "~2.3.0",
		"nette/forms": "~2.3.0",
		"nette/utils": "~2.3.0"
	},
	"suggest": {
		"nette/di": "Required to register the renderer via DI extension.",
		"nette/bootstrap": "Required to register the renderer via DI extension."
	},
	"require-dev": {
		"nette/bootstrap": "~2.3.0",
		"nette/di": "~2.3.0",
		"nette/http": "~2.3.0",
		"nette/mail": "~2.3.0",
		"nette/robot-loader": "~2.3.0",
		"nette/safe-stream": "~2.3.0",
		"tracy/tracy": "~2.3.0",
		"nette/tester": "~2.0.2",
		"php-parallel-lint/php-parallel-lint": "~1.4.0"
	},
	"autoload": {
		"psr-4": {
			"Kdyby\\BootstrapFormRenderer\\": "src/Kdyby/BootstrapFormRenderer/"
		}
	},
	"scripts": {
		"lint": "parallel-lint -e php,phpt --exclude vendor .",
		"test": "tester -c ./tests/php-tests.ini ./tests/KdybyTests/",
		"test:info": "tester -c ./tests/php-tests.ini ./tests/KdybyTests/ --info",
		"test:junit": "tester -c ./tests/php-tests.ini -o junit ./tests/KdybyTests/",
		"test:clean": "rm -rf ./temp/cache/*",
		"check": [
			"@lint",
			"@test"
		]
	}
}
